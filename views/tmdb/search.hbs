<ul>
    {{#if user}}
    <p>{{ user }}</p>
    {{/if }}
    {{#each searchObject.body.results }}
    {{#if media_type }}
    <li>{{#if title }}{{ title }}{{else}}{{ name }}{{/if}} <small>{{media_type}}</small></li>
    {{#if ../user }}
    <form action="{{../user.towatchList }}/add/{{ media_type }}/{{ id }}" method="POST">
        <button>Add to Watchlist</button>
    </form>
    {{/if}}
    {{/if}}
    {{/each}}
</ul>